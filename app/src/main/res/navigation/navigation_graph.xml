    android:id="@+id/navigation_graph"
    app:startDestination="@id/movieListFragment">
    <fragment
        android:id="@+id/movieListFragment"
        android:name="com.example.marcinwisniewski.intivetrainingapp.movielist.view.fragment.MovieListFragment"
        android:label="MovieListFragment">
        <action
            android:id="@+id/action_movieListFragment_to_detailsFragment"
            app:destination="@id/movieDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/movieDetailsFragment"
        android:name="com.example.marcinwisniewski.intivetrainingapp.moviedetails.view.fragment.MovieDetailsFragment"
        android:label="MovieDetailsFragment" />
</navigation>